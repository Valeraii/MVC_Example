@{
    ViewData["Title"] = "Files";
    string[]? fileNames = ViewData["fileNames"] as string[];
}

<div class="text-center">
    <h2>@ViewData["Title"]</h2>
    <ul>
        @foreach (var item in fileNames!)
        {
            <li>
                <a asp-controller="File" asp-action="Content"
                    asp-route-id="@item">@item</a>
            </li>
        }
    </ul>
</div>